{
  "name": "contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "t": "forge test",
    "t:v": "forge test -v",
    "t:vv": "forge test -vv",
    "t:vvv": "forge test -vvv",
    "t:vvvv": "forge test -vvvv",
    "t:w": "forge test --watch",
    "t:g": "forge test --gas-report --match-path src/gas/\\*/\\*.t.sol",
    "test": "npx hardhat test",
    "test:fast": "cross-env TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=1 npx hardhat test",
    "test:watch": "hardhat watch test",
    "test:gas": "cross-env REPORT_GAS=true npx hardhat test",
    "mocha:watch": " mocha -r ts-node/register --watch --watch-files  test/indepTests.ts",
    "coverage": "npx hardhat coverage",
    "compile": "npx hardhat compile",
    "lint": "npx eslint '**/*.js'",
    "lint:sol": "prettier --list-different 'contracts/**/*.sol'",
    "lint:fix": "npx eslint '**/*.js' --fix",
    "prettier": "npx prettier '**/*.{json,sol,md}' --check",
    "prettier:sol": "prettier --write contracts/**/*.sol",
    "prettier:fix": "npx prettier '**/*.{json,sol,md}' --write",
    "solhint": "npx solhint 'contracts/**/*.sol'",
    "solhint:fix": "npx solhint 'contracts/**/*.sol' --fix",
    "accounts": "npx hardhat accounts",
    "deploy": "echo NODE_ENV is DEV #node scripts/deploy.js",
    "deploy:ropsten": "echo NODE_ENV is DEV #node scripts/deploy.js --network ropsten",
    "deploy:kovan": "echo NODE_ENV is DEV #node scripts/deploy.js --network kovan",
    "deploy:rinkeby": "echo NODE_ENV is DEV #node scripts/deploy.js --network rinkeby",
    "soldoc": "npx soldoc docs/ contracts/parachutes/Parachutes.sol",
    "docgen": "solidity-docgen -i contracts/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "dotenv": "^16.0.1",
    "eslint-config-standard": "^17.0.0",
    "glob": "^7.2.0",
    "hardhat": "^2.9.5"
  },
  "devDependencies": {
    "@defi-wonderland/smock": "^2.0.7",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-ganache": "^2.0.1",
    "@nomiclabs/hardhat-solpp": "^2.0.1",
    "@tenderly/hardhat-tenderly": "^1.0.13",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "docgen": "^0.0.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.5",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-dependency-compiler": "^1.1.2",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-watcher": "^2.3.0",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solidity": "^0.0.1",
    "solidity-coverage": "^0.7.0",
    "solidity-docgen": "^0.6.0-beta.10",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.4"
  }
}
